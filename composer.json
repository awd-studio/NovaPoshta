{
    "name": "awd-studio/novaposhta",
    "description": "PHP SDK for Nova Poshta API",
    "keywords": [
        "delivery",
        "delivery-api",
        "nova",
        "novaposhta",
        "novaposhta-api",
        "poshta",
        "post",
        "postservices",
        "sdk",
        "ukraine",
        "wrapper-api"
    ],
    "homepage": "https://github.com/awd-studio/novaposhta",
    "type": "library",
    "require": {
        "php": ">=5.5 | 7",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7",
        "php-coveralls/php-coveralls": "^2.0@dev"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Anton Karpov",
            "email": "awd.com.ua@gmail.com",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/awd-studio/novaposhta/issues",
        "source": "https://github.com/awd-studio/novaposhta"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "NP\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NP\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "coverage": "vendor/bin/phpunit -c ./phpunit.xml.dist",
        "coveralls": "php vendor/bin/coveralls -v"
    }
}
